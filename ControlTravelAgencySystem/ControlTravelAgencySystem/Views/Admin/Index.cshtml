@model ControlTravelAgencySystem.Models.ViewModels.AdminPanelPageView

@{
    ViewBag.Title = "Панель управления";
}

@if (ViewBag.access)
{
    @section Styles {
        <link href="@Href("~/Styles/ControlPanel.css")" rel="stylesheet" type="text/css" />
    }

    @section scripts {
        <script src="@Href("~/Scripts/ControlPanel.js")"></script>
    }

    @RenderPage("~/views/admin/ControlPanel.cshtml")
}
else
{
    @section Styles {
        <link href="@Href("~/Styles/Auth.css")" rel="stylesheet" type="text/css" />
    }

    @RenderPage("~/views/admin/Auth.cshtml")
}