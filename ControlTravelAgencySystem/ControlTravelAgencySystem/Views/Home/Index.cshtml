@model ControlTravelAgencySystem.Models.ViewModels.ToursView
@{
    ViewBag.Title = "Туристическое бюро";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="tab-content">
    <div class="tab-pane fade in active" id="custom-tour-tab">

        <div id="tours-title">
            <div class="text-center">
                <h3>Добро пожаловать в туристическое бюро!</h3>
            </div>
            <br />
            <div class="float-left">
                <h4>Выберите понравившиеся Вам туры:</h4>
            </div>
            <div class="float-right">
                <button type="button" class="btn btn-primary" onclick="location.href='@Url.Action("FavoriteList", "Rooms")'">Список пожеланий</button>
            </div>
        </div>
        <div id="tours-list">
            @Html.Action("GetToursList", "Tours")
        </div>

    </div>

    <div class="tab-pane fade" id="completed-tour-tab">
        TEST
    </div>
</div>